{% comment %}
    This template will be automatically generated for you if the file doesn't exist,
    but where's the fun in having no control? Define your layout below.

    The 'layout settings.customer_layout' line is inserted at the top of every customer account template.
    Your theme's settings determine to use the 'default' or 'theme' customer templates.
    More info:
    - http://www.tetchi.ca/shopify-theme-from-scratch-part-11/

    Template note:
        - All IDs on this template are required to make the form(s) submit properly

    For all available customer liquid tags:
     - http://docs.shopify.com/themes/liquid-variables/customer
{% endcomment %}

{{ 'customer.css' | asset_url | stylesheet_tag }}

<style>
  .account-page .halo-page-content{
    padding-bottom: 0;
  }
  .account-page.my-account-page {
    background: #fff;
    height: 100%;
  }
  .custom_tabs .col-md-12 nav{
    border-right: 1px solid #e3e3e3;
    margin-right: 40px;
  }
  .custom_tabs .col-md-12 nav ul{
    border: 1px solid #e3e3e3;
    margin-right: 50px;
  }
  .custom_tabs .col-md-12 nav ul>.nav-item{
    border-bottom: 1px solid #e3e3e3;
  }
  .custom_tabs .col-md-12 nav ul>.nav-item:last-of-type{
    border-bottom: 0;
  }
  .custom_tabs .col-md-12 nav ul>.nav-item a{
    padding: 20px;
    text-align: left;
  }
  .custom_tabs .col-md-12 nav ul>.nav-item a.active{
    background: #f1f1f1;
  }
  .custom_tabs .col-md-12 nav ul>.nav-item.active a{
    background: #f1f1f1;
  }
  .top-sec{
    margin: 0 0 20px;
  }
  .top-sec .box-wrap{
    background: #fee9e8;
    padding: 30px 20px;
    margin: 0;
  }
  .top-sec .box-wrap .left-txt p{
    font-size: 16px;
    margin: 0;
  }
  .top-sec .box-wrap .left-txt p span{
    color: #87c644;
  }
  .top-sec .box-wrap .btn{
    width: 100%;
    max-width: 90px;
    font-size: 15px;
  }
  .top-sec .box-wrap .btn-outline{
    background: transparent;
    border: 1px solid gray;
  }
  .top-sec .box-wrap .btn-success{
    background: #fee9e8;
    border-color: #fee9e8;
    color: #000;
  }
  .top-sec .name-wrap p{
    font-size: 16px;
    margin: 20px 0;
  }
  .top-sec .name-wrap p span, .top-sec .name-wrap p a{
    color: #a39590;
    text-decoration: none;
  }
  .cards ul{
    border: 0;
    display: flex;
    flex-wrap: wrap;
    margin: 0 -10px;
  }
  .cards ul li{
    width: 33.33%;
    padding: 10px;
    text-align: center;
    border-bottom: 0;
  }
  .cards ul li a{
    border: 1px solid #e3e3e3 !important;
    box-shadow: 0 0 8px #d5d5d5;
    font-size: 16px;
    text-transform: uppercase;
    padding: 10px 1rem;
  }
  .cards ul li a img{
    margin: 0 auto;
    width: 100%;
    height: auto;
    max-width: 50px;
  }
  .top-sec.desk-hide{
    display: none;
  }
  .whishlist-contnt{
    border: 1px solid #e3e3e3 !important;
    box-shadow: 0 0 8px #d5d5d5;
    font-size: 16px;
    text-transform: uppercase;
    padding: 10px 1rem;
    width: 100%;
    max-width: 300px;
    text-align: center;
    border-radius: 0.25rem;
  }
  .whishlist-contnt img{
    margin: 0 auto;
    width: 100%;
    height: auto;
    max-width: 50px;
  }
  .custom_tabs .tab-content .inner_col {
    padding-block: 15px;
    height: 100%;
  }
  .custom_tabs .tab-content .inner_col .in_box{
    border: 1px solid #e3e3e3 !important;
    box-shadow: 0 0 8px #d5d5d5;
    font-size: 16px;
    text-transform: uppercase;
    padding: 20px;
    height: 100%;
  }
  .cards.card_hide{
    display: none;
  }

  @media(max-width: 767px){
    .custom_account {
      padding-top: 50px;
    }
    .custom_tabs nav {
      border-right: 0 !important;
      margin-right: 0 !important;
    }
    .custom_tabs .col-md-12 nav ul {
      margin-right: 0;
    }
    .custom_tabs .col-md-12 nav ul>.nav-item a {
      font-size: 13px;
      line-height: 18px;
      padding: 20px 10px;
    }
    .custom_tabs .col-md-12 .tab-content{
      padding-top: 0 !important;
    }
    .custom_tabs .col-md-12 .tab-content .tab-pane {
      padding: 0 !important;
    }
    .top-sec .box-wrap .text-right{
      margin: 20px 0 0;
      text-align: center !important;
    }
    .top-sec .box-wrap .btn-outline{
      margin-right: 0 !important;
    }    
    .cards ul {
      margin: 0 -5px;
      justify-content: center;
    }
    .cards ul li {
      width: 50%;
      padding: 5px;
    }
    .cards ul li a{
      font-size: 12px;
      font-weight: 600;
      line-height: 18px;
      height: 100%;
      box-shadow: none;
    }
    .top-sec.mob-hide{
      display: none;
    }
    .top-sec.desk-hide{
      display: block;
    }
    .cards ul li a img{
      max-width: 40px;
    }
    .custom_tabs .col-md-12 .tab-content h2,
    .custom_tabs .col-md-12 .tab-content .tab-pane h1{
      font-size: 25px;
    }
    .custom_tabs .col-md-12 .tab-content .tab-pane div,
    .custom_tabs .col-md-12 .tab-content .tab-pane p{
      font-size: 14px;    
    }

    .yotpo-coupons-redemption-widget .yotpo-redemption-options-list {display:block !important;}
    .yotpo-coupons-redemption-widget .three-in-row .yotpo-redemption-item {width: 100% !important;}


  }
      .customerHelp li {margin: 0 0 15px;}
</style>

